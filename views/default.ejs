<!DOCTYPE html>
<html>
<head>
	<title>My webpage OpenSource</title>
	<meta charset="utf-8">
</head>
<body>
	<div class="container">
		<h1>User layout</h1>
		<h3>Bitte Eingloggen</h3>
		<table class="table table-hover">
	      <thead>
	        <tr>
	          <th>#</th>
	          <th>titel</th>
	          <th>interpret</th>
	          <th>jahr</th>
	        </tr>
	      </thead>
	      <tbody>
	      	<% for (var i=0;i<rows.length;i++){ %>
	        <tr>
	          <td scope="row"><%= rows[i].id %></td>
	          <td><%= rows[i].titel %></td>
	          <td><%= rows[i].interpret %></td>
	          <td><%= rows[i].jahr %></td>
	        </tr>
	        <% } %>
	      </tbody>
	    </table>
	</div>
<link rel="stylesheet" href="css/bootstrap.min.css">
<link rel="stylesheet" href="css/bootstrap-theme.min.css">
<script src="js/bootstrap.min.js"></script>
</body>
</html>